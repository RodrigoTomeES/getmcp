{
  "name": "@mcp-hub/generators",
  "version": "0.1.0",
  "description": "Config generators that transform canonical MCP server definitions to app-specific formats",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
