  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-hub/mcp-hub.git",
    "directory": "packages/generators"
  },
  "homepage": "https://github.com/mcp-hub/mcp-hub/tree/main/packages/generators#readme",
  "bugs": {
    "url": "https://github.com/mcp-hub/mcp-hub/issues"
  },
  "engines": {
    "node": ">=18.0.0"
